//this file specifies all of the callback functions each interaction event triggers
//pass the url of this file to the constructor of P5InteractionManager along with the object that contains these functions
{
		"click->leftMouse" :  "add_polygon_on_edge",
		"click->rightMouse":  "remove_selected_polygon",
		"mousePress->leftMouse":  "remove_selected_polygon",
		"mouseMove": "select_polygon",

		"release->delete":    "remove_selected_polygon",
		"press->space":     "add_polygon_on_edge",
		"press->a":         "add_polygon_on_edge",
		"press->command&d": "remove_selected_polygon",

	}
}